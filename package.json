{
  "name": "hexo-csp",
  "version": "0.4.1",
  "description": "A hexo plugin to generate and inject a CSP.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "test": "jest",
    "hash": "scripts/hash"
  },
  "jest": {
    "testRegex": "/__tests__/.*.spec.js",
    "testPathIgnorePatterns": [
      "support/",
      "__snapshots__"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ],
    "testTimeout": 60000,
    "restoreMocks": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xbc5/hexo-csp.git"
  },
  "keywords": [
    "hexo",
    "plugin",
    "csp",
    "content security policy",
    "security"
  ],
  "author": "xbc5 <87829033+xbc5@users.noreply.github.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xbc5/hexo-csp/issues"
  },
  "homepage": "https://github.com/xbc5/hexo-csp",
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "joi": "^17.6.0",
    "lodash.clonedeep": "^4.5.0",
    "body-parser": "^1.20.0",
    "node-forge": "^1.3.1"
  },
  "peerDependencies": {
    "hexo": "^6.2.0"
  },
  "devDependencies": {
    "@fsouza/prettierd": "^0.21.0",
    "fast-check": "^3.0.0",
    "hexo-renderer-markdown-it": "^6.0.1",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-test-utils": "^0.6.0",
    "jest": "^29.2.0",
    "jest-serializer-html": "^7.1.0"
  }
}
